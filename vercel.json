{
  "functions": {
    "src/app/api/version/route.ts": {
      "runtime": "edge"
    },
    "src/app/api/user-count/route.ts": {
      "runtime": "edge"
    }
  },
  "regions": ["iad1"],
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "installCommand": "npm ci",
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=3600, s-maxage=3600"
        }
      ]
    }
  ]
}