# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url_here
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Rate Limiting (Optional - for production)
UPSTASH_REDIS_REST_URL=your_upstash_redis_url_here
UPSTASH_REDIS_REST_TOKEN=your_upstash_redis_token_here

# Optional: For development
NODE_ENV=development

# Application Configuration
NEXT_PUBLIC_APP_NAME=Nightreign Seed Finder
NEXT_PUBLIC_APP_VERSION=1.0.0

# GitHub Pages base URLs
# Example repo: https://artimuz.github.io/Nightreign-Seed-Finder
NEXT_PUBLIC_PAGES_BASE_URL=
NEXT_PUBLIC_PAGES_PUBLIC_BASE_URL=

# Rollback flag: disable serving chunks/assets from GitHub Pages
# Set to true to force serving from Vercel.
NEXT_PUBLIC_DISABLE_PAGES_CHUNKS=

# Legacy (kept for backward compatibility)
NEXT_PUBLIC_PAGES_ASSET_BASE_URL=

# Security Configuration
SECURITY_ENABLE_RATE_LIMITING=true
SECURITY_LOG_LEVEL=info